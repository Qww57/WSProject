<?xml version="1.0" encoding="UTF-8" ?>
<process
    name="travelGoodBPEL"
    targetNamespace="http://enterprise.netbeans.org/bpel/TravelGoodBPEL/travelGoodBPEL"
    xmlns:tns="http://enterprise.netbeans.org/bpel/TravelGoodBPEL/travelGoodBPEL"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2"
    xmlns:sxat="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Attachment"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:ns0="http://j2ee.netbeans.org/wsdl/NiceView/java/NiceView" xmlns:ns1="http://j2ee.netbeans.org/wsdl/TravelGoodBPEL/src/TravelGoodWSDL">
    <import namespace="http://j2ee.netbeans.org/wsdl/LameDuckWS/LameDuckWS/LameDuck" location="http://localhost:8080/LameDuck/LameDuckService?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/NiceView/java/NiceView" location="http://localhost:8080/NiceView/NiceViewService?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://types.fastmoney.imm.dtu.dk" location="http://localhost:8080/NiceView/NiceViewService?xsd=1" importType="http://www.w3.org/2001/XMLSchema"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/TravelGoodBPEL/src/TravelGoodWSDL" location="TravelGoodWSDL.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="PartnerLink3" xmlns:tns="http://j2ee.netbeans.org/wsdl/TravelGoodBPEL/src/TravelGoodWSDL" partnerLinkType="tns:TravelGoodWSDL" myRole="TravelGoodWSDLPortTypeRole"/>
        <partnerLink name="LameDuck" xmlns:tns="http://j2ee.netbeans.org/wsdl/LameDuckWS/LameDuckWS/LameDuck" partnerLinkType="tns:LameDuck" partnerRole="LameDuckPortTypeRole"/>
        <partnerLink name="NiceView" xmlns:tns="http://j2ee.netbeans.org/wsdl/NiceView/java/NiceView" partnerLinkType="tns:NiceView" partnerRole="NiceViewPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="CreateItineraryOut" messageType="ns1:createItineraryResponse"/>
        <variable name="CreateItineraryIn" messageType="ns1:createItineraryRequest"/>
        <variable name="restartItinerary" type="xs:boolean"/>
        <variable name="itineraryListHotelCounter" type="xs:int"/>
        <variable name="itineraryListFlightCounter" type="xs:int"/>
        <variable name="itineraryList" type="ns1:PlanOutputType">
            <sxed:editor>
                <sxed:predicate path="$itineraryList/HotelsPlanInformation[$itineraryListHotelCounter]" source="to"/>
                <sxed:predicate path="$itineraryList/FlightsPlanInformation[$itineraryListFlightCounter]" source="to"/>
                <sxed:predicate path="$itineraryList/HotelsPlanInformation[$planHotelsCounter]" source="from"/>
                <sxed:predicate path="$itineraryList/FlightsPlanInformation[$planFlightCounter]" source="from"/>
            </sxed:editor>
        </variable>
        <variable name="planHotelsCounter" type="xs:int"/>
        <variable name="planFlightCounter" type="xs:int"/>
        <variable name="PlanFlightsAndHotelsOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/TravelGoodBPEL/src/TravelGoodWSDL" messageType="tns:PlanFlightsAndHotelsResponse">
            <sxed:editor>
                <sxed:predicate path="$PlanFlightsAndHotelsOut.part1/HotelsPlanInformation[$planHotelsCounter]" source="to"/>
            </sxed:editor>
        </variable>
        <variable name="PlanFlightsAndHotelsIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/TravelGoodBPEL/src/TravelGoodWSDL" messageType="tns:PlanFlightsAndHotelsRequest">
            <sxed:editor>
                <sxed:predicate path="$PlanFlightsAndHotelsIn.part1/HotelsBookingNumber[$planHotelsCounter]" source="from"/>
                <sxed:predicate path="$PlanFlightsAndHotelsIn.part1/FlightsBookingNumber[$planFlightCounter]" source="from"/>
            </sxed:editor>
        </variable>
        <variable name="GetFlightCounter" type="xs:int"/>
        <variable name="GetHotelCount" type="xs:int"/>
        <variable name="GetFlightsAndHotelsOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/TravelGoodBPEL/src/TravelGoodWSDL" messageType="tns:GetFlightsAndHotelsResponse">
            <sxed:editor>
                <sxed:predicate path="$GetFlightsAndHotelsOut.part1/HotelsList/HotelInformations[$GetHotelCount]" source="to"/>
                <sxed:predicate path="$GetFlightsAndHotelsOut.part1/HotelsList[$GetHotelCount]" source="to"/>
                <sxed:predicate path="$GetFlightsAndHotelsOut.part1/FlightsList[$GetFlightCounter]" source="to"/>
            </sxed:editor>
        </variable>
        <variable name="GetFlightsAndHotelsIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/TravelGoodBPEL/src/TravelGoodWSDL" messageType="tns:GetFlightsAndHotelsRequest">
            <sxed:editor>
                <sxed:predicate path="$GetFlightsAndHotelsIn.part1/HotelRequests/HotelsList[$GetHotelCount]" source="from"/>
                <sxed:predicate path="$GetFlightsAndHotelsIn.part1/FlightRequests/FlightsList[$GetFlightCounter]" source="from"/>
            </sxed:editor>
        </variable>
    </variables>
    <correlationSets>
        <correlationSet name="ItineraryCorrelation" properties="ns1:ID"/>
    </correlationSets>
    <sequence>
        <receive name="Receive3" createInstance="yes" partnerLink="PartnerLink3" operation="createItinerary" portType="ns1:TravelGoodWSDLPortType" variable="CreateItineraryIn">
            <correlations>
                <correlation set="ItineraryCorrelation" initiate="join"/>
            </correlations>
        </receive>
        <assign name="Assign8">
            <copy>
                <from>true()</from>
                <to>$CreateItineraryOut.part1/ItineraryCreatedBoolean</to>
            </copy>
            <copy>
                <from variable="CreateItineraryIn" property="ns1:ID"/>
                <to>$CreateItineraryOut.part1/ItineraryID</to>
            </copy>
        </assign>
        <reply name="Reply3" partnerLink="PartnerLink3" operation="createItinerary" portType="ns1:TravelGoodWSDLPortType" variable="CreateItineraryOut">
        </reply>
        <receive name="Receive1" createInstance="no" partnerLink="PartnerLink3" operation="GetFlightsAndHotels" xmlns:tns="http://j2ee.netbeans.org/wsdl/TravelGoodBPEL/src/TravelGoodWSDL" portType="tns:TravelGoodWSDLPortType" variable="GetFlightsAndHotelsIn">
            <correlations>
                <correlation set="ItineraryCorrelation" initiate="no"/>
            </correlations>
        </receive>
        <assign name="Assign3">
            <copy>
                <from>1</from>
                <to variable="GetHotelCount"/>
            </copy>
            <copy>
                <from>1</from>
                <to variable="GetFlightCounter"/>
            </copy>
        </assign>
        <forEach name="getHotelsLoop" parallel="no" counterName="ForEach1Counter">
            <startCounterValue>1</startCounterValue>
                <finalCounterValue>count($GetFlightsAndHotelsIn.part1/HotelRequests/HotelsList)</finalCounterValue>
                <scope name="Scope1">
                    <variables>
                            <variable name="GetHotelsOut" messageType="ns0:getHotelsResponse"/>
                                <variable name="GetHotelsIn" messageType="ns0:getHotelsRequest"/>
                        </variables>
                        <sequence name="Sequence1">
                            <assign name="Assign1">
                                    <copy>
                                            <from>$GetFlightsAndHotelsIn.part1/HotelRequests/HotelsList[$GetHotelCount]
                                <sxed:editor>
                                                            <sxed:predicate path="$GetFlightsAndHotelsIn.part1/HotelRequests/HotelsList[$GetHotelCount]" source="from"/>
                                                        </sxed:editor>
                                                </from>
                                                <to variable="GetHotelsIn" part="hotelsRequest"/>
                                        </copy>
                                        <copy>
                                            <from>$GetFlightsAndHotelsIn.part1/HotelRequests/HotelsList[$GetHotelCount]/City
                                <sxed:editor>
                                                            <sxed:predicate path="$GetFlightsAndHotelsIn.part1/HotelRequests/HotelsList[$GetHotelCount]" source="from"/>
                                                        </sxed:editor>
                                                </from>
                                                <to>$GetHotelsIn.hotelsRequest/City</to>
                                        </copy>
                                        <copy>
                                            <from>$GetFlightsAndHotelsIn.part1/HotelRequests/HotelsList[$GetHotelCount]/ArrivalDate
                                <sxed:editor>
                                                            <sxed:predicate path="$GetFlightsAndHotelsIn.part1/HotelRequests/HotelsList[$GetHotelCount]" source="from"/>
                                                        </sxed:editor>
                                                </from>
                                                <to>$GetHotelsIn.hotelsRequest/ArrivalDate</to>
                                        </copy>
                                        <copy>
                                            <from>$GetFlightsAndHotelsIn.part1/HotelRequests/HotelsList[$GetHotelCount]/DepartureDate
                                <sxed:editor>
                                                            <sxed:predicate path="$GetFlightsAndHotelsIn.part1/HotelRequests/HotelsList[$GetHotelCount]" source="from"/>
                                                        </sxed:editor>
                                                </from>
                                                <to>$GetHotelsIn.hotelsRequest/DepartureDate</to>
                                        </copy>
                                </assign>
                                <invoke name="Invoke3" partnerLink="NiceView" operation="getHotels" portType="ns0:NiceViewPortType" inputVariable="GetHotelsIn" outputVariable="GetHotelsOut"/>
                                <assign name="Assign2">
                                            <copy>
                                                <from>$GetHotelsOut.hotels/HotelInformations</from>
                                                    <to>$GetFlightsAndHotelsOut.part1/HotelsList[$GetHotelCount]/HotelInformations
                                <sxed:editor>
                                                                <sxed:predicate path="$GetFlightsAndHotelsOut.part1/HotelsList[$GetHotelCount]" source="to"/>
                                                            </sxed:editor>
                                                    </to>
                                            </copy>
                            </assign>
                                <assign name="Assign7">
                                <copy>
                                    <from>$GetHotelCount + 1</from>
                                    <to variable="GetHotelCount"/>
                                </copy>
                            </assign>
                        </sequence>
                </scope>
        </forEach>
        <forEach name="getFlightsLoop" parallel="no" counterName="ForEach2Counter" xmlns:tns="http://j2ee.netbeans.org/wsdl/LameDuckWS/LameDuckWS/LameDuck">
            <startCounterValue>1</startCounterValue>
                <finalCounterValue>count($GetFlightsAndHotelsIn.part1/FlightRequests/FlightsList)</finalCounterValue>
                <scope name="Scope2">
                    <variables>
                        <variable name="GetFlightsOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/LameDuckWS/LameDuckWS/LameDuck" messageType="tns:getFlightsResponse"/>
                        <variable name="GetFlightsIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/LameDuckWS/LameDuckWS/LameDuck" messageType="tns:getFlightsRequest"/>
                    </variables>
                        <sequence name="Sequence2">
                        <assign name="Assign4">
                            <copy>
                                <from>$GetFlightsAndHotelsIn.part1/FlightRequests/FlightsList[$GetFlightCounter]/start
                                    <sxed:editor>
                                        <sxed:predicate path="$GetFlightsAndHotelsIn.part1/FlightRequests/FlightsList[$GetFlightCounter]" source="from"/>
                                    </sxed:editor>
                                </from>
                                <to>$GetFlightsIn.getFlightsInput/start</to>
                            </copy>
                            <copy>
                                <from>$GetFlightsAndHotelsIn.part1/FlightRequests/FlightsList[$GetFlightCounter]/destination
                                    <sxed:editor>
                                        <sxed:predicate path="$GetFlightsAndHotelsIn.part1/FlightRequests/FlightsList[$GetFlightCounter]" source="from"/>
                                    </sxed:editor>
                                </from>
                                <to>$GetFlightsIn.getFlightsInput/destination</to>
                            </copy>
                            <copy>
                                <from>$GetFlightsAndHotelsIn.part1/FlightRequests/FlightsList[$GetFlightCounter]/date
                                    <sxed:editor>
                                        <sxed:predicate path="$GetFlightsAndHotelsIn.part1/FlightRequests/FlightsList[$GetFlightCounter]" source="from"/>
                                    </sxed:editor>
                                </from>
                                <to>$GetFlightsIn.getFlightsInput/date</to>
                            </copy>
                        </assign>
                        <invoke name="Invoke2" partnerLink="LameDuck" operation="getFlights" xmlns:tns="http://j2ee.netbeans.org/wsdl/LameDuckWS/LameDuckWS/LameDuck" portType="tns:LameDuckPortType" inputVariable="GetFlightsIn" outputVariable="GetFlightsOut"/>
                        <assign name="Assign5">
                            <copy>
                                <from>$GetFlightsOut.getFlightsOutput/FlightInformations</from>
                                <to>$GetFlightsAndHotelsOut.part1/FlightsList[$GetFlightCounter]/FlightInformations
                                    <sxed:editor>
                                        <sxed:predicate path="$GetFlightsAndHotelsOut.part1/FlightsList[$GetFlightCounter]" source="to"/>
                                    </sxed:editor>
                                </to>
                            </copy>
                        </assign>
                        <assign name="Assign6">
                            <copy>
                                <from>$GetFlightCounter + 1</from>
                                <to variable="GetFlightCounter"/>
                            </copy>
                        </assign>
                    </sequence>
                </scope>
        </forEach>
        <reply name="Reply1" partnerLink="PartnerLink3" operation="GetFlightsAndHotels" xmlns:tns="http://j2ee.netbeans.org/wsdl/TravelGoodBPEL/src/TravelGoodWSDL" portType="tns:TravelGoodWSDLPortType" variable="GetFlightsAndHotelsOut"/>
        <receive name="Receive2" partnerLink="PartnerLink3" operation="PlanFlightsAndHotels" xmlns:tns="http://j2ee.netbeans.org/wsdl/TravelGoodBPEL/src/TravelGoodWSDL" portType="tns:TravelGoodWSDLPortType" createInstance="no" variable="PlanFlightsAndHotelsIn">
            <correlations>
                <correlation set="ItineraryCorrelation" initiate="no"/>
            </correlations>
        </receive>
        <assign name="planCounterReset">
            <copy>
                <from>1</from>
                <to variable="planHotelsCounter"/>
            </copy>
            <copy>
                <from>1</from>
                <to variable="planFlightCounter"/>
            </copy>
        </assign>
        <forEach name="addHotelsToItinerary" parallel="no" counterName="ForEach3Counter">
            <startCounterValue>1</startCounterValue>
            <finalCounterValue>count($PlanFlightsAndHotelsIn.part1/HotelsBookingNumber)</finalCounterValue>
            <scope name="Scope3">
                <sequence name="Sequence3">
                    <assign name="addHotelsToItinerary">
                        <copy>
                                <from>$PlanFlightsAndHotelsIn.part1/HotelsBookingNumber[$planHotelsCounter]
                                            <sxed:editor>
                                                <sxed:predicate path="$PlanFlightsAndHotelsIn.part1/HotelsBookingNumber[$planHotelsCounter]" source="from"/>
                                            </sxed:editor>
                                    </from>
                                    <to>$itineraryList/HotelsPlanInformation[$itineraryListHotelCounter]/BookingNumber
                                            <sxed:editor>
                                                <sxed:predicate path="$itineraryList/HotelsPlanInformation[$itineraryListHotelCounter]" source="to"/>
                                            </sxed:editor>
                                    </to>
                            </copy>
                            <copy>
                                <from>'unconfirmed'</from>
                                    <to>$itineraryList/HotelsPlanInformation[$itineraryListHotelCounter]/Status
                                            <sxed:editor>
                                                <sxed:predicate path="$itineraryList/HotelsPlanInformation[$itineraryListHotelCounter]" source="to"/>
                                            </sxed:editor>
                                    </to>
                            </copy>
                    </assign>
                    <assign name="increasePlanHotelCounters">
                        <copy>
                            <from>$itineraryListHotelCounter + 1</from>
                            <to variable="itineraryListHotelCounter"/>
                        </copy>
                        <copy>
                            <from>1 + $planHotelsCounter</from>
                            <to variable="planHotelsCounter"/>
                        </copy>
                    </assign>
                </sequence>
            </scope>
        </forEach>
        <forEach name="addFlightsToItinerary" parallel="no" counterName="ForEach4Counter">
            <startCounterValue>1</startCounterValue>
            <finalCounterValue>count($PlanFlightsAndHotelsIn.part1/FlightsBookingNumber)</finalCounterValue>
            <scope name="Scope4">
                <sequence name="Sequence4">
                    <assign name="addFlightsToItinerary">
                        <copy>
                                <from>$PlanFlightsAndHotelsIn.part1/FlightsBookingNumber[$planFlightCounter]
                                            <sxed:editor>
                                                <sxed:predicate path="$PlanFlightsAndHotelsIn.part1/FlightsBookingNumber[$planFlightCounter]" source="from"/>
                                            </sxed:editor>
                                    </from>
                                    <to>$itineraryList/FlightsPlanInformation[$itineraryListFlightCounter]/BookingNumber
                                            <sxed:editor>
                                                <sxed:predicate path="$itineraryList/FlightsPlanInformation[$itineraryListFlightCounter]" source="to"/>
                                            </sxed:editor>
                                    </to>
                            </copy>
                            <copy>
                                <from>'unconfirmed'</from>
                                    <to>$itineraryList/FlightsPlanInformation[$itineraryListFlightCounter]/Status
                                            <sxed:editor>
                                                <sxed:predicate path="$itineraryList/FlightsPlanInformation[$itineraryListFlightCounter]" source="to"/>
                                            </sxed:editor>
                                    </to>
                            </copy>
                    </assign>
                    <assign name="increasePlanFlightCounters">
                        <copy>
                            <from>1 + $planFlightCounter</from>
                            <to variable="planFlightCounter"/>
                        </copy>
                        <copy>
                            <from>1 + $itineraryListFlightCounter</from>
                            <to variable="itineraryListFlightCounter"/>
                        </copy>
                    </assign>
                </sequence>
            </scope>
        </forEach>
        <assign name="planCounterResetAgain">
            <copy>
                <from>1</from>
                <to variable="planHotelsCounter"/>
            </copy>
            <copy>
                <from>1</from>
                <to variable="planFlightCounter"/>
            </copy>
        </assign>
        <forEach name="itineraryHotelsToOutput" parallel="no" counterName="ForEach5Counter">
            <startCounterValue>1</startCounterValue>
            <finalCounterValue>count($itineraryList/HotelsPlanInformation)</finalCounterValue>
            <scope name="Scope5">
                <sequence name="Sequence5">
                    <assign name="addHotelToPlanOutput">
                        <copy>
                            <from>$itineraryList/HotelsPlanInformation[$planHotelsCounter]
                                <sxed:editor>
                                    <sxed:predicate path="$itineraryList/HotelsPlanInformation[$planHotelsCounter]" source="from"/>
                                </sxed:editor>
                            </from>
                            <to>$PlanFlightsAndHotelsOut.part1/HotelsPlanInformation[$planHotelsCounter]
                                <sxed:editor>
                                    <sxed:predicate path="$PlanFlightsAndHotelsOut.part1/HotelsPlanInformation[$planHotelsCounter]" source="to"/>
                                </sxed:editor>
                            </to>
                        </copy>
                    </assign>
                    <assign name="incrHotelCounter">
                        <copy>
                            <from>1 + $planHotelsCounter</from>
                            <to variable="planHotelsCounter"/>
                        </copy>
                    </assign>
                </sequence>
            </scope>
        </forEach>
        <forEach name="itineraryFlightsToOutput" parallel="no" counterName="ForEach6Counter">
            <startCounterValue>1</startCounterValue>
            <finalCounterValue>count($itineraryList/FlightsPlanInformation)</finalCounterValue>
            <scope name="Scope6">
                <sequence name="Sequence6">
                    <assign name="addFlightToPlanOutput">
                        <copy>
                                <from>$itineraryList/FlightsPlanInformation[$planFlightCounter]
                                            <sxed:editor>
                                                <sxed:predicate path="$itineraryList/FlightsPlanInformation[$planFlightCounter]" source="from"/>
                                            </sxed:editor>
                                    </from>
                                    <to>$PlanFlightsAndHotelsOut.part1/FlightsPlanInformation[$planFlightCounter]
                                            <sxed:editor>
                                                <sxed:predicate path="$PlanFlightsAndHotelsOut.part1/FlightsPlanInformation[$planFlightCounter]" source="to"/>
                                            </sxed:editor>
                                    </to>
                            </copy>
                    </assign>
                    <assign name="incrFlightCounter">
                        <copy>
                            <from>1 + $planFlightCounter</from>
                            <to variable="planFlightCounter"/>
                        </copy>
                    </assign>
                </sequence>
            </scope>
        </forEach>
        <reply name="Reply2" partnerLink="PartnerLink3" operation="PlanFlightsAndHotels" xmlns:tns="http://j2ee.netbeans.org/wsdl/TravelGoodBPEL/src/TravelGoodWSDL" portType="tns:TravelGoodWSDLPortType" variable="PlanFlightsAndHotelsOut"/>
    </sequence>
</process>
